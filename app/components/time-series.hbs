<div class="timeseries__container" ...attributes>
  <h5 class="timeseries__title">{{@title}}</h5>
  <ApexChart
    class="timeseries__chart"
    @type="line"
    @width={{@width}}
    @height={{@height}}
    @series={{this.series}}
    @chartOptions={{this.chartOptions}}
  />
</div>

